{
  "name": "GenSim",
  "version": "0.4.0",
  "description": "Multiple interface generic HMI simulator tool",
  "keywords":["generic","simulator","interfaces","hmi"],

  "main":"html/index.html",
  "single-instance":true,
  "gypfile": true,

  "config":{
    "engine-strict":true
  },
  "os" : ["win32", "linux"],
  "cpu" : ["ia32"],
  "engines" : { "node" : "0.10.22" },
  "engines" : { "npm" : "1.3.14" },

  "dependencies": {
    "bootstrap":"4.1.1",
    "fs-extra":"6.0.1",
    "xml2js":"0.4.19",
    "bindings": "~1.2.1",
    "d3":"3.5.5",
    "jquery":"3.3.1",
    "arraylist":"0.1.0",
    "mongoose":"3.8.24",
    "jsonfile":"4.0.0",
    "node-pre-gyp":"0.6.4",
    "semver":"4.1.0",
    "binary-pack":"0.0.3",
    "bufferutil":"1.0.1",
    "utf-8-validate":"1.0.1",
    "ws":"0.7.1",
    "primus":"2.4.12",
    "python-shell":"0.0.3",
    "tree-node":"0.2.0",
    "deepcopy":"0.5.0",
    "serialport":"1.4.9",
    "node-can-master":"2.0.1"
  },

  "window": {
    "title":"GENSIM V0.4.0",
    "toolbar": true,
    "resizable": true,
    "position": "mouse"
  },

  "maintainers": [
    {
      "name": "Gert Claassen",
      "email": "gertclaassen@outlook.com",
      "url": ""
    }
  ],

  "repository":
